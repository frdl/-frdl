{
  "name": "frdl",
  "description": "frdl.js",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "frdl",
    "frdlweb",
    "require-browser",
    "webfan",
    "development"
  ],
  "homepage": "https://github.com/frdl/frdl.js",
  "version": "2.0.18",
  "author": {
    "name": "Till Wehowski",
    "email": "github@webfan.de",
    "url": "http://frdl.webfan.de"
  },
  "contributors": [],
  "config": {
    "frdlweb": {}
  },
  "engines": {
    "node": ">=9.10.1",
    "npm": ">=5.4.0",
    "yarn": ">=1.9.0"
  },
  "main": "index",
  "bin": "./bin/frdl",
  "browserify": {
    "transform": "deamdify"
  },
  "browser": {
    "fs": "./assets/node_modules/fs.js",
    "amdefine": "./assets/node_modules/amdefine.js"
  },
  "pkg": {
    "assets": [
      "assets/**/**",
      "docs/**/**"
    ],
    "targets": [
      "host",
      "linux",
      "macos",
      "win",
      "node9",
      "node10"
    ]
  },
  "scripts": {
    "build": "npm run build:browser-lib",
    "build:browser-lib": "rimraf ./dist && mkdir ./dist && node ./build.js && rimraf ./public/httpdocs/test && rimraf ./public/httpdocs/dist && rimraf ./public/httpdocs/js && cp -rf ./dist/ ./public/httpdocs",
    "node": "node",
    "start": "node ./server.js",
    "frdl": "node ./bin/frdl",
    "rimraf": "rimraf",
    "yarn": "yarn",
    "npm": "npm",
    "prepublishOnly": "npm run package:refresh && npm version patch",
    "pre-install": "npm run package:refresh",
    "post-install": "npm link && yarn",
    "post-install:legacy": "node ./scripts/build-legacy.js legacy-build",
    "package:refresh": "rimraf ./.env && rimraf ./node_modules && rimraf ./dist && rimraf ./package-lock.json && rimraf ./yarn.lock && rimraf ./npm-debug.log && rimraf ./yarn-debug.log && rimraf ./yarn-error.log && rimraf ./.nyc_output && rimraf ./modules",
    "installEX": "scripts/installEx.js"
  },
  "repository": "frdl/frdl.js",
  "bugs": "http://github.com/frdl/frdl.js/issues",
  "workspaces": {
    "default": [
      "**/workspace/**"
    ],
    "hps": [
      "**/.hps/**/app/**",
      "**/.hps/**/modules/**",
      "**/.hps/**/var/**",
      "**/.hps/**/node_modules/**",
      "**/.hps/**/app/**",
      "**/.hps/**/config/**"
    ],
    "blog": [
      "**/.hps/**"
    ],
    "webfan": [
      "**/.uid/**/WORKSPACE/**"
    ],
    "vhost": [
      "**/_hosts/**"
    ],
    "nohoist": [
      ".hps/**",
      "**/.hps/**",
      "**/frdl/**",
      "**/frdlweb/**",
      "**/server-frdl-de/**",
      "**/git/**",
      "**/weakref/**",
      "node_modules/**",
      "modules/**"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-es2015": "^7.0.0-beta.53",
    "@babel/preset-es2016": "^7.0.0-beta.53",
    "@frdl/eventemitter": ">=2.0.1",
    "@frdl/functions": ">=0.0.3",
    "@frdl/node-browser-shim": "latest",
    "@frdl/process": "*",
    "@frdl/requirejs": ">=4.0.3",
    "@frdl/vm": ">=1.0.1",
    "@frdl/webfan": ">=3.0.11",
    "amdefine": "^1.0.1",
    "axios": "^0.18.0",
    "babel-loader": "^8.0.5",
    "babel-preset-es2015": "^6.24.1",
    "browserify": "^16.2.3",
    "chalk": "*",
    "clone": "^2.1.2",
    "co": "*",
    "co-prompt": "*",
    "connect": "*",
    "copy-webpack-plugin": "^5.0.3",
    "deamdify": "^0.3.0",
    "debug": "^4.1.1",
    "dotenv": "*",
    "dynamic-cdn-webpack-plugin": "^4.0.0", 
    "edit-json-file": "^1.2.1",
    "expose-loader": "^0.7.5",
    "feature": "^1.1.0",
    "file-loader": "^3.0.1",
    "fs-extra": "*",
    "html-webpack-plugin": "^3.2.0",
    "imports-loader": "^0.8.0",
    "jayson": "*",
    "jszip": "^3.1.5",
    "machina": "^4.0.2",
    "mimemessage": "^1.0.5",
    "module-to-cdn": "^3.1.2",
    "node-php": "0.0.1",
    "optparse": "^1.0.5",
    "php-core": "*",
    "php-parser": "*",
    "php-reflection": "*",
    "php-require": "^0.0.2",
    "php-runtime": "*",
    "raw-loader": "^2.0.0",
    "ready-check": "*",
    "remote-webpack-plugin": "^1.0.2", 
     "requirejs-to-webpack-cli": "^1.0.0", 
    "requirejs-webpack-plugin": "^1.0.5",
    "resolve": "*",
    "save-remote-file-webpack-plugin": "^1.0.2",
    "script-loader": "^0.7.2",
    "shelljs": "^0.8.3",
    "simple-jsonrpc-js": "0.0.10",
    "subarg": "^1.0.0",
    "try-require": "^1.2.1",
    "val-loader": "^1.1.1",
    "webpack": "^4.30.0", 
    "webpack-require-http": "^0.4.3"
  },
  "devDependencies": {
    "pkg": "^4.3.8",
    "pm2": "*",
    "rimraf": "^2.6.2",
    "superagent": "*",
    "tap": "^13.1.6",
    "yarn": "^1.12.3"
  },
  "peerDependencies": {},
  "licenses": [
    {
      "type": "MIT",
      "url": "http://www.opensource.org/licenses/mit-license.php"
    }
  ]
}
