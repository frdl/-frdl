{
  "name": "frdl",
  "description": "frdl.js - Frdlweb frameworks node/javascript part of the developer-workspace edition.",
  "type": "library",
  "license": "MIT",
  "version": "4.0.2-beta",	  
  "keywords": [
    "frdl",
    "frdlweb",
    "require-browser",
    "webfan",
    "development",
    "project-management"
  ],
  "homepage": "https://frdl.webfan.de/install/npm/", 
  "repository" : {
    "type" : "git",
    "url": "ssh://git@github.com/frdl/frdl.js.git"
  },
  "publishConfig": { 	
	  "registry": "https://npm.pkg.github.com/"
   },	  
  "author": {
    "name": "Till Wehowski",
    "email": "github@webfan.de",
    "url": "http://frdl.webfan.de"
  },
  "contributors": [
  
  ],
  "config": {
    "frdlweb": {}
  },
  "bin": {
    "frdljs": "./frdl",
    "frdl": "./frdl",
	"req": "./bin/req",
	"install-remote-files" : "./bin/frdl-install-remote-files"
  },
  "browser": {
    "fs": "./assets/node_modules/fs.js",
    "amdefine": "./assets/node_modules/amdefine.js"
  },
  "resolutions": {
    "terser": "3.14.1"
  },
  "scripts": {
    "build": "npm run build:install-remote-files && npm run build:browser-lib",
    "build:browser-lib": "npm run frdl bundlejs",
    "build:install-remote-files": "npm run frdl install-remote-files ./ --file './frdlweb-config.json'",
    "node": "node",
    "start": "node ./server.js",
    "frdl": "node ./bin/frdl",
    "rimraf": "rimraf",
    "yarn": "yarn",
    "npm": "npm",
    "prepublishOnly:legacy": "npm run package:refresh && npm version patch",
    "pre-install:legacy": "npm run package:refresh",
    "postinstall": "npm link",
    "post-install": "npm link",
    "preinstall": "npx npm-force-resolutions && npm run preinstall:prepare-install",
    "post-install:legacy": "node ./scripts/build-legacy.js legacy-build",
    "package:refresh": "npm uninstall && rimraf ./.env && rimraf ./node_modules && rimraf ./dist && rimraf ./package-lock.json && rimraf ./yarn.lock && rimraf ./npm-debug.log && rimraf ./yarn-debug.log && rimraf ./yarn-error.log && rimraf ./.nyc_output && rimraf ./modules",
    "install-mono": "scripts/installEx.js",
    "preinstall:prepare-install": "node ./prepare-install.js"
  },
  "bugs": "http://github.com/frdl/frdl/issues",
