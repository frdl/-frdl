{
  "name": "frdl",
  "description": "frdl.js",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "frdl",
    "frdlweb",
    "require-browser"
  ],
  "bin": {
    "frdl": "./bin/frdl"
  },
  "homepage": "https://github.com/frdl/frdl.js",
  "version": "2.0.13",
  "author": {
    "name": "Till Wehowski",
    "email": "github@webfan.de",
    "url": "http://frdl.webfan.de"
  },
  "contributors": [],
  "config": {
    "HPS_ID": "0",
    "APP_ID": "0"
  },
  "engines": {
    "node": ">=9.10.1",
    "npm": ">=5.4.0",
    "yarn": ">=1.9.0"
  },
  "main": "index",
  "scripts": {
    "node": "node",
    "start": "node ./server.js",
    "frdl": "node ./bin/frdl",
    "rimraf": "rimraf",
    "yarn": "yarn",
    "npm": "npm",
    "test": "tap test/",
    "prepublishOnly": "npm run package:refresh && npm version patch",
    "pre-install": "mkdir ./dist",
    "post-install": "yarn && npm link",
    "post-install:legacy": "node ./scripts/build-legacy.js legacy-build",
    "package:refresh": "rimraf ./.env && rimraf ./node_modules && rimraf ./dist && rimraf ./package-lock.json && rimraf ./yarn.lock && rimraf ./npm-debug.log && rimraf ./yarn-debug.log && rimraf ./yarn-error.log",
    "installEX": "scripts/installEx.js"
  },
  "repository": "frdl/frdl.js",
  "bugs": "http://github.com/frdl/frdl.js/issues",
  "workspaces": {
    "hps": [
      "**/.hps/**/app/**",
      "**/.hps/**/modules/**",
      "**/.hps/**/var/**",
      "**/.hps/**/node_modules/**",
      "**/.hps/**/app/**"
    ],
    "webfan": [
      "**/.uid/**/WORKSPACE/**"
    ],
    "vhost": [
      "**/_hosts/**"
    ],
    "nohoist": [
      "**/frdl/**",
      "**/frdlweb/**",
      "**/server-frdl-de/**",
      "**/git/**",
      "**/weakref/**",
      "node_modules/**"
    ]
  },
  "dependencies": {
    "ack-host": "*",
    "amdefine": "^1.0.1",
    "angular": "~1.3.8",
    "angular-terminal": "1.0.0",
    "angular-translate": "2.18.1",
    "axios": "^0.18.0",
    "chalk": "*",
    "co": "*",
    "co-prompt": "*",
    "connect": "*",
    "dotenv": "*",
    "dotenv-webpack": "*",
    "edit-json-file": "^1.2.1",
    "express": "*",
    "fs-extra": "*",
    "jayson": "*",
    "jquery-mousewheel": "*",
    "jquery.terminal": "*",
    "jszip": "^3.1.5",
    "optparse": "^1.0.5",
    "shelljs": "^0.8.3",
    "superagent": "*",
    "try-require": "^1.2.1",
    "twig": "*",
    "vhost": "*",
    "yarn": "^1.12.3"
  },
  "devDependencies": {
    "rimraf": "^2.6.2"
  },
  "peerDependencies": {},
  "licenses": [
    {
      "type": "MIT",
