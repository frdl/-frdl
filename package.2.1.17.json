{
  "name": "frdl",
  "description": "frdl.js",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "frdl",
    "frdlweb",
    "require-browser",
    "webfan",
    "development"
  ],
  "homepage": "https://github.com/frdl/frdl.js",
  "version": "2.1.17",
  "author": {
    "name": "Till Wehowski",
    "email": "github@webfan.de",
    "url": "http://frdl.webfan.de"
  },
  "contributors": [],
  "config": {
    "frdlweb": {}
  },
  "engines": {
    "node": ">=9.10.1",
    "npm": ">=5.4.0",
    "yarn": ">=1.9.0"
  },
  "main": "index",
  "bin": {
    "frdl": "./bin/frdl",
    "frdlweb": "./bin/frdlweb"
  },
  "browserify": {
    "transform": "deamdify"
  },
  "browser": {
    "fs": "./assets/node_modules/fs.js",
    "amdefine": "./assets/node_modules/amdefine.js"
  },
  "pkg": {
    "assets": [
      "assets/**/**",
      "docs/**/**"
    ],
    "targets": [
      "host",
      "linux",
      "macos",
      "win",
      "node9",
      "node10"
    ]
  },
  "scripts": {
    "build": "npm run build:browser-lib",
    "build:browser-lib": "npm run frdl bundlejs",
    "node": "node",
    "start": "node ./server.js",
    "frdl": "node ./bin/frdl --scripts-prepend-node-path",
    "rimraf": "rimraf",
    "yarn": "yarn",
    "npm": "npm",
    "prepublishOnly": "npm run package:refresh && npm version patch",
    "pre-install": "npm run package:refresh",
    "post-install": "npm link && yarn",
    "post-install:legacy": "node ./scripts/build-legacy.js legacy-build",
    "package:refresh": "npm uninstall && rimraf ./.env && rimraf ./node_modules && rimraf ./dist && rimraf ./package-lock.json && rimraf ./yarn.lock && rimraf ./npm-debug.log && rimraf ./yarn-debug.log && rimraf ./yarn-error.log && rimraf ./.nyc_output && rimraf ./modules",
    "installEX": "scripts/installEx.js"
  },
  "repository": "frdl/frdl.js",
  "bugs": "http://github.com/frdl/frdl.js/issues",
  "workspaces": {
    "default": [
      "**/workspace/**"
    ],
    "hps": [
      "**/.hps/**/app/**",
      "**/.hps/**/modules/**",
      "**/.hps/**/var/**",
      "**/.hps/**/node_modules/**",
      "**/.hps/**/app/**",
      "**/.hps/**/config/**"
    ],
    "blog": [
      "**/.hps/**"
    ],
    "webfan": [
      "**/.uid/**/WORKSPACE/**"
    ],
    "vhost": [
      "**/_hosts/**"
    ],
    "nohoist": [
      ".hps/**",
      "**/.hps/**",
      "**/frdl/**",
      "**/frdlweb/**",
      "**/server-frdl-de/**",
      "**/git/**",
      "**/weakref/**",
      "node_modules/**",
      "modules/**"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.6.0",
    "@babel/plugin-syntax-dynamic-import": "latest",
    "@babel/preset-env": "^7.6.0",
    "@babel/runtime": "^7.6.0",
    "@frdl/eventemitter": "^2.0.4",
    "@frdl/fs": "latest",
    "@frdl/functions": "latest",
    "@frdl/helper": "latest",
    "@frdl/magic-overload": "latest",
    "@frdl/node-browser-shim": "latest",
    "@frdl/process": "latest",
    "@frdl/requirejs": "latest",
    "@frdl/simple-downloader": "latest",
    "@frdl/sort": "latest",
    "@frdl/vm": "latest",
    "@frdl/webfan": "latest",
    "amdefine": "^1.0.1",
    "async-context": "^1.0.0",
    "axios": "^0.18.1",
    "babel-loader": "^8.0.5",
    "babel-minify-webpack-plugin": "^0.3.1",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "chalk": "*",
    "clean-webpack-plugin": "^2.0.2",
    "clone": "^2.1.2",
    "co": "*",
    "co-prompt": "*",
    "connect": "^3.7.0",
    "copy-webpack-plugin": "^5.0.3",
    "debug": "^4.1.1",
    "dotenv": "*",
    "edit-json-file": "^1.2.1",
    "expose-loader": "^0.7.5",
    "extensioner": "^1.18.4",
    "feature": "^1.1.0",
    "file-loader": "^3.0.1",
    "fs-extra": "^8.1.0",
    "html-webpack-plugin": "^3.2.0",
    "imports-loader": "^0.8.0",
    "inline-worker": "^1.1.0",
    "insertion-query": "1.0.4",
    "jayson": "*",
    "jszip": "^3.2.2",
    "mimemessage": "^1.0.5",
    "module-to-cdn": "^3.1.2",
    "npm-install-webpack-plugin": "^4.0.5",
    "optparse": "^1.0.5",
    "php-parser": "*",
    "php-reflection": "*",
    "raw-loader": "^2.0.0",
    "ready-check": "latest",
    "recursive-readdir": "^2.2.2",
    "requirejs-webpack-plugin": "^1.0.5",
    "resolve": "^1.11.0",
    "save-remote-file-webpack-plugin": "^1.0.2",
    "script-loader": "^0.7.2",
    "shelljs": "^0.8.3",
    "simple-jsonrpc-js": "0.0.10",
    "sliced": "^1.0.1",
    "subarg": "^1.0.0",
    "try-require": "^1.2.1",
    "uglify-js": "^3.6.0",
    "uglifyjs-browser": "^3.0.0",
    "url": "0.11.0",
    "util": "0.12.0",
    "val-loader": "^1.1.1",
    "webpack": "^4.40.0",
    "webpack-inject-plugin": "^1.5.0",
    "webpack-pwa-manifest": "^4.0.0",
    "webpack-shell-plugin": "^0.5.0"
  },
  "devDependencies": {
    "rimraf": "^2.6.2",
    "yarn": "^1.12.3"
  },
  "peerDependencies": {},
  "licenses": [
    {
      "type": "MIT",
      "url": "http://www.opensource.org/licenses/mit-license.php"
    }
  ]
}
