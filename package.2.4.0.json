{
  "name": "frdl",
  "description": "frdl.js",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "frdl",
    "frdlweb",
    "require-browser",
    "webfan",
    "development"
  ],
  "homepage": "https://webfan.de/install/npm/",
  "version": "2.5.0",
  "author": {
    "name": "Till Wehowski",
    "email": "github@webfan.de",
    "url": "http://frdl.webfan.de"
  },
  "contributors": [],
  "config": {
    "frdlweb": {}
  },
  "main": "index",
  "bin": {
    "frdl": "./bin/frdl",
    "frdlweb": "./bin/frdlweb"
  },
  "browser": {
    "fs": "./assets/node_modules/fs.js",
    "amdefine": "./assets/node_modules/amdefine.js"
  },
  "scripts": {
    "build": "npm run build:browser-lib",
    "build:browser-lib": "npm run frdl bundlejs",
    "node": "node",
    "start": "node ./server.js",
    "frdl": "node ./bin/frdl --scripts-prepend-node-path",
    "rimraf": "rimraf",
    "yarn": "yarn",
    "npm": "npm",
    "prepublishOnly": "npm run package:refresh && npm version patch",
    "pre-install:legacy": "npm run package:refresh",
    "post-install": "npm link",
    "pre-install": "npm run pre-install:prepare-install",
    "post-install:legacy": "node ./scripts/build-legacy.js legacy-build",
    "package:refresh": "npm uninstall && rimraf ./.env && rimraf ./node_modules && rimraf ./dist && rimraf ./package-lock.json && rimraf ./yarn.lock && rimraf ./npm-debug.log && rimraf ./yarn-debug.log && rimraf ./yarn-error.log && rimraf ./.nyc_output && rimraf ./modules",
    "installEX": "scripts/installEx.js",
    "pre-install:prepare-install": "node ./prepare-install.js"
  },
  "repository": "frdl/frdl.js",
  "bugs": "http://github.com/frdl/frdl.js/issues",
